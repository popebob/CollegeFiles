                <xsd:element name="PetName" type="xsd:string"/>
                <xsd:element name="TypeOfPet" type="xsd:string"/>
                <xsd:element name="AdditionalInformation" type="xsd:string"/>  	
        </xsd:all>
    </xsd:complexType>
    
</xsd:schema>